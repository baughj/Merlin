{"sEcho": <%= params[:sEcho] || -1 %>,
 "iTotalRecords" : <%= @total_objects %>,
 "iTotalDisplayRecords" : <%= @total_objects %>,
 "aaData" : [<% @objects.each do |object| %>
  ["<%= object.status_code %>",
   "<%= object.hostname %>",
   "<%= object.instance_id %>",
   "<%= object.vm_type.name %>",
   "<% object.security_groups.each do |secgroup| %> <%= secgroup.name %><% end %>", 
   "<% if object.key_pair %><%= object.key_pair.name %><% else %>unknown<% end %>",
   "<%= object.status_message %>",
  ],
  <% end %>
]}
